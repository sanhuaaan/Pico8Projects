pico-8 cartridge // http://www.pico-8.com
version 5
__lua__
--music(1,0,1)

playing=false
function _init()
 poke(0x5f2c,3)
 timer=0 
 px=28
 py=56
 pspr=0
 pstate=0 --0:idle, 1:walking, 2:covering, 3:dead  
 singing=true
 --tune=flr(rnd(3)+1)
 tune=4
 numdrops=30
 dropx={numdrops}
 dropy={numdrops}
 droptype={numdrops}
 dropspeed={numdrops}
 maxnotes=3
 currentnote=1
 notesx={3}
 notesy={3}
 for n=1,maxnotes do
 	notesx[n]=0
 	notesy[n]=-20
 end 
 for i=0,numdrops do
 	dropx[i]=rnd(64)
 	dropy[i]=rnd(64)*-1
 	droptype[i]=flr(rnd(2))
 	dropspeed[i]=rnd(1)
 end
end

function _update()
 if not singing then
 	music(-1)
 	singing=false
 end
 if playing then 
	 timer+=1
	 control_gremlin()
	 acidize_rain() 
	 for i=0,numdrops do 	
 	 if dropy[i] > 64 then 	  
	   dropx[i]=rnd(64)-4
	  	dropy[i]=rnd(64)*-1
	  else
	  	dropy[i]+=(1+dropspeed[i])
	  end
  end
  for n=1,maxnotes do
  	notesy[n] -=1
  end
  if pstate<2 and timer%30==0 then
 	 notesx[currentnote]=px
 	 notesy[currentnote]=py-8
 	 currentnote+=1
 	 if currentnote > maxnotes then
 	  currentnote=1
 	 end 	
  end
  
  if timer%15==0 then
 	 if sget(0,5) == 0 then
 	  sset(0,5,6)
 	  sset(0,6,0)
 	  sset(6,6,6)
  	 sset(6,5,0)
 		else
 	  sset(0,6,6)
 	  sset(0,5,0)
 	  sset(6,5,6)
 	  sset(6,6,0)
 	 end
 	end
 else
  if btn(5) then
   _init()
   playing=true
   music(tune,0,1)
  end
 end	  
end

function _draw()
 cls() 
 if playing then
  for i=0,numdrops do  
   if pstate==2 and 
  	 		dropx[i] < (px + 3) and
  		 	dropx[i] > (px - 12) and
  			 dropy[i] > 50 and
  			 dropy[i] < 54 then
  	 spr(8,dropx[i],dropy[i]-rnd(3)-1)
  	 dropx[i]=rnd(64)-4
	 	 dropy[i]=rnd(64)*-1
	 	 sfx(1)
   end
   spr(5+droptype[i],dropx[i],dropy[i],1,1)
  end
  if pstate==2 then
   sspr(72,0,16,16,px-8,py-8)
  elseif pstate==1 then
   pspr=flr(timer/3)%2+1 
   spr(pspr,px,py,1,1)
  elseif pstate==3 then
 	 spr(3,px,py,1,1)
 	 print("never give up",6,20,8)
  else
   spr(0,px,py,1,1)
  end  
  for n=1,maxnotes do
   spr(16,notesx[n]+(sin(timer/10)),notesy[n],1,1)
  end
 else
  spr(0,28,56)
  print("singing in the",3,20,7)
  print("acid rain",12,29,rnd(16))
 end  	  
end

function control_gremlin()
 if pstate < 3 then
  pstate=0
  if btn(4) then
   pstate=2
   if singing then   
    --tune=flr(rnd(3)+1)
    tune=4
    singing=false 	
   end      
  elseif (btn(0) and px>-1) then
   pstate=1
	 	px-=1
	 	if not singing then
	 	 music(tune,0,1)
	 	 singing=true
	 	end
  elseif (btn(1) and px<58) then
   pstate=1
 	 px+=1
 	 if not singing then
 	  music(tune,0,1)
	 	 singing=true
	 	end 
  end
 else
 	if btn(5) then
   _init()
   music(tune,0,1)
  end
 end 
 for i=0,numdrops do  
  if pstate<2 and 
  			dropx[i] < (px + 2) and
  			dropx[i] > (px - 4) and
  			dropy[i] > (py-2) and
  			dropy[i] < 57 then
  	pstate=3
  	singing=false
  	dropx[i]=rnd(64)-4
	 	dropy[i]=rnd(64)*-1
	 	sfx(0)
  end
 end 
end

function acidize_rain()
	acidize1=rnd(16)
	acidize2=rnd(16)
	acidize3=rnd(16)
	sset(58,4,acidize1)--1
	sset(34,2,acidize1)--2
	sset(37,2,acidize1)
	sset(34,5,acidize1)
	sset(37,5,acidize1)
	sset(35,3,acidize2)
	sset(35,4,acidize2)
	sset(36,3,acidize2)
	sset(36,4,acidize2)
	sset(43,3,acidize3)--3
	sset(45,3,acidize3)
	sset(44,4,acidize2)
	sset(43,5,acidize3)
	sset(45,5,acidize3)
	sset(51,4,acidize2)--4
	sset(52,3,acidize2)
	sset(52,5,acidize2)
	sset(52,4,acidize3)
	sset(53,4,acidize2)
end
